{
  "name": "agost",
  "version": "0.0.1",
  "description": "Fast, unopinionated, minimalist web framework for Node.js based on Express",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run check && tsc && npm run lint",
    "test": "jest",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "check": "prettier --ignore-path .gitignore -c \"**/*.+(js|ts|json)\"",
    "lint": "eslint --ignore-path .gitignore \"**/*.+(js|ts|json)\""
  },
  "author": "Willian Carlos Agostini",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.5.9"
  },
  "devDependencies": {
    "@swc/core": "^1.3.74",
    "@swc/jest": "^0.2.28",
    "@tsconfig/node18": "^18.2.0",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "node-mocks-http": "^1.13.0",
    "prettier": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "engineStrict": true,
  "keywords": [
    "express",
    "framework",
    "sinatra",
    "web",
    "http",
    "rest",
    "restful",
    "router",
    "app",
    "api"
  ],
  "bugs": {
    "url": "https://github.com/WillianAgostini/agost/issues"
  },
  "homepage": "https://github.com/WillianAgostini/agost#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WillianAgostini/agost.git"
  }
}
